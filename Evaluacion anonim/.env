# === MySQL (contenedor Docker) ===
DB_HOST=127.0.0.1
DB_PORT=3306
DB_USER=gdpr_ro
DB_PASS=ro_pass
DB_NAME=sakila_es

# === Ollama (servidor local) ===
OLLAMA_URL=http://localhost:11434/api/generate
OLLAMA_MODEL=llama3.2:3b-instruct-q4_K_M

# === Parámetros por defecto de auto_request_from_mysql.py ===
# Patrón LIKE para seleccionar tablas (ej.: %, cliente%, fact%)
TABLE_LIKE=%
# Nº de muestras (no nulas) por columna para evidencias en el prompt
SAMPLE_ROWS=5
# Nº de columnas por petición al LLM
BATCH_SIZE=20
# Pausa entre peticiones al LLM (segundos)
SLEEP_S=0.3
# Fichero de salida con predicciones
OUT_PREDICTIONS=predictions.json
